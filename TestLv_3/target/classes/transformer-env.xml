<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!--  指定mysql 的链接信息  -->
<configuration>
	<property>
		<name>mysql.batch.number</name>
		<value>500</value>
	</property>

	<property>
		<name>mysql.report.driver</name>
		<value>com.mysql.jdbc.Driver</value>
	</property>

	<property>
		<name>mysql.report.url</name>
		<value>jdbc:mysql://192.168.4.1:3306/report?useUnicode=true&amp;characterEncoding=utf8</value>
	</property>

	<property>
		<name>mysql.report.username</name>
		<value>root</value>
	</property>

	<property>
		<name>mysql.report.password</name>
		<value>ls515011</value>
	</property>
</configuration>
